                                                                                          [ rdf:type owl:Restriction ;
                                                                                            owl:onProperty <http://www.nimble-project.org/ns/securitypolicyvocab#appliesTo> ;
                                                                                            owl:someValuesFrom <http://www.nimble-project.org/ns/securitypolicyvocab#Data>
                                                                                          ] ,
                                                                                          [ rdf:type owl:Restriction ;
                                                                                            owl:onProperty <http://www.nimble-project.org/ns/securitypolicyvocab#flowsTo> ;
                                                                                            owl:someValuesFrom [ rdf:type owl:Class ;
                                                                                                                 owl:unionOf ( <http://www.nimble-project.org/ns/securitypolicyvocab#Actor>
                                                                                                                               <http://www.nimble-project.org/ns/securitypolicyvocab#Asset>
                                                                                                                             )
                                                                                                               ]
                                                                                          ] .



###  http://www.nimble-project.org/ns/securitypolicyvocab#InvocationSecure

<http://www.nimble-project.org/ns/securitypolicyvocab#InvocationSecure> rdf:type owl:Class ;
                                                                                      
                                                                                      rdfs:subClassOf <http://www.nimble-project.org/ns/securitypolicyvocab#State> .



###  http://www.nimble-project.org/ns/securitypolicyvocab#InvocationUnsecure

<http://www.nimble-project.org/ns/securitypolicyvocab#InvocationUnsecure> rdf:type owl:Class ;
                                                                                        
                                                                                        rdfs:subClassOf <http://www.nimble-project.org/ns/securitypolicyvocab#State> .



###  http://www.nimble-project.org/ns/securitypolicyvocab#Message

<http://www.nimble-project.org/ns/securitypolicyvocab#Message> rdf:type owl:Class ;
                                                                             
                                                                             rdfs:subClassOf <http://www.nimble-project.org/ns/securitypolicyvocab#Data> .



###  http://www.nimble-project.org/ns/securitypolicyvocab#MessageContent

<http://www.nimble-project.org/ns/securitypolicyvocab#MessageContent> rdf:type owl:Class ;
                                                                                    
                                                                                    rdfs:label "MessageContent" ;
                                                                                    
                                                                                    rdfs:subClassOf <http://www.nimble-project.org/ns/securitypolicyvocab#Data> ;
                                                                                    
