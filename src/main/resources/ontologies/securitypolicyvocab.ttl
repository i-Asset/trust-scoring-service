                                                                                                         owl:onProperty <http://www.nimble-project.org/ns/securitypolicyvocab#appliesTo> ;
                                                                                                         owl:someValuesFrom <http://www.nimble-project.org/ns/securitypolicyvocab#Asset>
                                                                                                       ] ,
                                                                                                       [ rdf:type owl:Restriction ;
                                                                                                         owl:onProperty <http://www.nimble-project.org/ns/securitypolicyvocab#hasPolicy> ;
