<http://www.nimble-project.org/ns/securitypolicyvocab#memberOf> rdf:type owl:ObjectProperty ;
                                                                              
                                                                              rdfs:domain <http://www.nimble-project.org/ns/securitypolicyvocab#User> ;
                                                                              
                                                                              rdfs:range <http://www.nimble-project.org/ns/securitypolicyvocab#UserGroup> .



###  http://www.nimble-project.org/ns/securitypolicyvocab#ownedBy

<http://www.nimble-project.org/ns/securitypolicyvocab#ownedBy> rdf:type owl:ObjectProperty ;
                                                                             
                                                                             rdfs:comment "A property to establish ownership between a resource and its owner." .





#################################################################
#
#    Data properties
#
#################################################################


###  http://www.nimble-project.org/ns/securitypolicyvocab#hasName

<http://www.nimble-project.org/ns/securitypolicyvocab#hasName> rdf:type owl:DatatypeProperty ;
                                                                             
                                                                             rdfs:comment "anything can have a name. e.g file has a name 'XYZ'" .



###  http://www.nimble-project.org/ns/securitypolicyvocab#hasValue

<http://www.nimble-project.org/ns/securitypolicyvocab#hasValue> rdf:type owl:DatatypeProperty ;
                                                                              
                                                                              rdfs:domain <http://www.nimble-project.org/ns/securitypolicyvocab#Expression> .





#################################################################
#
#    Classes
#
#################################################################


###  http://www.nimble-project.org/ns/securitypolicyvocab#Actor

<http://www.nimble-project.org/ns/securitypolicyvocab#Actor> rdf:type owl:Class ;
                                                                           
                                                                           rdfs:label "Actor" ;
                                                                           
                                                                           rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                                             owl:onProperty <http://www.nimble-project.org/ns/securitypolicyvocab#hasAttribute> ;
                                                                                             owl:someValuesFrom <http://www.nimble-project.org/ns/securitypolicyvocab#Attribute>
                                                                                           ] ,
                                                                                           [ rdf:type owl:Restriction ;
                                                                                             owl:onProperty <http://www.nimble-project.org/ns/securitypolicyvocab#hasName> ;
                                                                                             owl:someValuesFrom xsd:string
                                                                                           ] ;
                                                                           
                                                                           rdfs:seeAlso "" ;
                                                                           
                                                                           rdfs:comment "An actor (or an agent) is a entity that is accessing to resources (i.e. assets) or wants to communicate with the resources." .



###  http://www.nimble-project.org/ns/securitypolicyvocab#Aplication

<http://www.nimble-project.org/ns/securitypolicyvocab#Aplication> rdf:type owl:Class ;
                                                                                
                                                                                rdfs:subClassOf <http://www.nimble-project.org/ns/securitypolicyvocab#Actor> ,
                                                                                                <http://www.nimble-project.org/ns/securitypolicyvocab#Asset> .



###  http://www.nimble-project.org/ns/securitypolicyvocab#Asset

<http://www.nimble-project.org/ns/securitypolicyvocab#Asset> rdf:type owl:Class ;
                                                                           
                                                                           rdfs:label "Asset" ;
                                                                           
                                                                           rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                                             owl:onProperty <http://www.nimble-project.org/ns/securitypolicyvocab#developedBy> ;
                                                                                             owl:someValuesFrom owl:Thing
                                                                                           ] ,
                                                                                           [ rdf:type owl:Restriction ;
                                                                                             owl:onProperty <http://www.nimble-project.org/ns/securitypolicyvocab#hasAttribute> ;
                                                                                             owl:allValuesFrom <http://www.nimble-project.org/ns/securitypolicyvocab#Attribute>
                                                                                           ] ,
                                                                                           [ rdf:type owl:Restriction ;
                                                                                             owl:onProperty <http://www.nimble-project.org/ns/securitypolicyvocab#hasName> ;
                                                                                             owl:someValuesFrom xsd:string
                                                                                           ] ,
                                                                                           [ rdf:type owl:Restriction ;
                                                                                             owl:onProperty <http://www.nimble-project.org/ns/securitypolicyvocab#ownedBy> ;
                                                                                             owl:someValuesFrom owl:Thing
                                                                                           ] ;
                                                                           
                                                                           rdfs:comment "An asset that is beign protected in terms of access control or controled in other relevant terms (e.g. security flows)." .



###  http://www.nimble-project.org/ns/securitypolicyvocab#Attribute

<http://www.nimble-project.org/ns/securitypolicyvocab#Attribute> rdf:type owl:Class ;
                                                                               
                                                                               rdfs:label "Attribute" ;
                                                                               
                                                                               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                                                 owl:onProperty <http://www.nimble-project.org/ns/securitypolicyvocab#hasValue> ;
                                                                                                 owl:someValuesFrom rdf:PlainLiteral
                                                                                               ] ,
                                                                                               [ rdf:type owl:Restriction ;
                                                                                                 owl:onProperty <http://www.nimble-project.org/ns/securitypolicyvocab#hasName> ;
                                                                                                 owl:someValuesFrom xsd:string
                                                                                               ] ;
                                                                               
                                                                               rdfs:comment "Any attribute that belongs to either actor or asset (e.g. location, reputation, trust). An attribute hasName and hasValue" .



###  http://www.nimble-project.org/ns/securitypolicyvocab#Authenticated

<http://www.nimble-project.org/ns/securitypolicyvocab#Authenticated> rdf:type owl:Class ;
                                                                                   
                                                                                   rdfs:subClassOf <http://www.nimble-project.org/ns/securitypolicyvocab#State> .



###  http://www.nimble-project.org/ns/securitypolicyvocab#Authorized

<http://www.nimble-project.org/ns/securitypolicyvocab#Authorized> rdf:type owl:Class ;
                                                                                
                                                                                rdfs:subClassOf <http://www.nimble-project.org/ns/securitypolicyvocab#State> .



###  http://www.nimble-project.org/ns/securitypolicyvocab#Data

<http://www.nimble-project.org/ns/securitypolicyvocab#Data> rdf:type owl:Class ;
                                                                          
                                                                          rdfs:subClassOf <http://www.nimble-project.org/ns/securitypolicyvocab#Asset> .



###  http://www.nimble-project.org/ns/securitypolicyvocab#Database

<http://www.nimble-project.org/ns/securitypolicyvocab#Database> rdf:type owl:Class ;
                                                                              
                                                                              rdfs:subClassOf <http://www.nimble-project.org/ns/securitypolicyvocab#StorageSystem> .



###  http://www.nimble-project.org/ns/securitypolicyvocab#DeniedAccess

<http://www.nimble-project.org/ns/securitypolicyvocab#DeniedAccess> rdf:type owl:Class ;
                                                                                  
                                                                                  rdfs:subClassOf <http://www.nimble-project.org/ns/securitypolicyvocab#State> .



###  http://www.nimble-project.org/ns/securitypolicyvocab#Device

<http://www.nimble-project.org/ns/securitypolicyvocab#Device> rdf:type owl:Class ;
                                                                            
                                                                            rdfs:subClassOf <http://www.nimble-project.org/ns/securitypolicyvocab#Actor> ,
                                                                                            <http://www.nimble-project.org/ns/securitypolicyvocab#Asset> .



###  http://www.nimble-project.org/ns/securitypolicyvocab#Effect

<http://www.nimble-project.org/ns/securitypolicyvocab#Effect> rdf:type owl:Class ;
                                                                            
                                                                            owl:equivalentClass <http://www.nimble-project.org/ns/securitypolicyvocab#State> .



###  http://www.nimble-project.org/ns/securitypolicyvocab#Encrypted

<http://www.nimble-project.org/ns/securitypolicyvocab#Encrypted> rdf:type owl:Class ;
                                                                               
                                                                               rdfs:subClassOf <http://www.nimble-project.org/ns/securitypolicyvocab#State> .



###  http://www.nimble-project.org/ns/securitypolicyvocab#EndUser

<http://www.nimble-project.org/ns/securitypolicyvocab#EndUser> rdf:type owl:Class ;
                                                                             
                                                                             rdfs:subClassOf <http://www.nimble-project.org/ns/securitypolicyvocab#User> .



###  http://www.nimble-project.org/ns/securitypolicyvocab#Expression

<http://www.nimble-project.org/ns/securitypolicyvocab#Expression> rdf:type owl:Class ;
                                                                                
                                                                                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                                                                  owl:onProperty <http://www.nimble-project.org/ns/securitypolicyvocab#hasValue> ;
                                                                                                  owl:someValuesFrom rdf:PlainLiteral
                                                                                                ] .



###  http://www.nimble-project.org/ns/securitypolicyvocab#File

<http://www.nimble-project.org/ns/securitypolicyvocab#File> rdf:type owl:Class ;
                                                                          
                                                                          rdfs:subClassOf <http://www.nimble-project.org/ns/securitypolicyvocab#StorageSystem> .



###  http://www.nimble-project.org/ns/securitypolicyvocab#Flow

<http://www.nimble-project.org/ns/securitypolicyvocab#Flow> rdf:type owl:Class ;
                                                                          
